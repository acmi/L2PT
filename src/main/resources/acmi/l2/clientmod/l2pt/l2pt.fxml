<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.BorderPane?>

<!--suppress JavaFxDefaultTag -->

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="acmi.l2.clientmod.l2pt.L2PT">
    <top>
        <MenuBar BorderPane.alignment="CENTER">
            <menus>
                <Menu mnemonicParsing="false" text="%file">
                    <items>
                        <MenuItem accelerator="CTRL+O" mnemonicParsing="false" text="%file.open.ini" />
                        <MenuItem mnemonicParsing="false" text="%file.open.folder" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem accelerator="ALT+F4" mnemonicParsing="false" onAction="#exit" text="%file.close" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="%help">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#showAbout" text="%help.about" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
</BorderPane>
